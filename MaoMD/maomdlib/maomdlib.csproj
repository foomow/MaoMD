<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Authors>MaoDaShu</Authors>
    <Product>MaoMDLib</Product>
    <Description>Generator of GitHub Flavored Markdown documentation for c# projects.
Works With the dll file and Xml documentation generated by Visual Studio.
Make all .md documents automatic…</Description>
    <Version>1.0.2</Version>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/foomow/MaoMD</PackageProjectUrl>
    <RepositoryUrl>https://github.com/foomow/MaoMD.git</RepositoryUrl>
    <RepositoryType>HTTPS</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <ApplicationIcon>maolib.ico</ApplicationIcon>
    <Company>MaoLib</Company>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>maomdlib.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
